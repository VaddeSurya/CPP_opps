//Private
#include<bits/stdc++.h>
using namespace std;
class Human{
private:
string name;
int age;
public:
    void setdata(int val, string word){
        age=val;
        name=word;
    }
    void getdata(){
        cout<<"Hi i am "<<name<<" my age is "<<age;
    }
};
int main()
{
    Human aa;
    aa.setdata(18,"ajay ");
    aa.getdata();
    return 0;
}
